<html t:type="layout" title="Person List | sylvarbo"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">

<div class="row">
    <div class="col-12">
        <h1>Person List</h1>
    </div>
</div>

<div class="row">
    <div class="col-12">
        <t:grid source="persistedObjects" row="currentObject" include="id" add="name">
         <p:nameCell>${currentObject.primaryIdentity.displayableName}</p:nameCell>
        </t:grid>

    </div>
</div>

 <t:form t:id="editForm">
  <t:select t:id="style"/>
  <t:submit value="Add"/>
 </t:form>

</html>
